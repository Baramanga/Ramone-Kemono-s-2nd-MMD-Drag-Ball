#Utensil
define utensilsettings(i)
	color (0.7,0.7,0.7)
	alpha 4
	size 1
	source -point -offset (0,0,0.4)
	inject 1 -hold 1	
	align zPole 
	life 2
	rotate 0.5
	aspect 1/9
enddef

particles fork
	create utensilsettings(0)
	texture RamoneFork -draw alphaTestDissolve  -sortOffset -0.01
end

effect fork -rigid -hardStop
	particles fork
end

metaparticles forkreoriented
	yaw 1
	roll 1 -vary 0.05
	pitch 1 -vary 0.05
	life 2
	inject 1 -hold 1
	source -point
	effect fork
	size 6
end

effect forkreoriented -rigid
	metaparticles forkreoriented
end

effect forktilted1 -rigid -hardStop
	effect forkreoriented -rotateXYZ 10 10 0
end

particles knife
	create utensilsettings(0)
	texture RamoneKnife -draw alphaTestDissolve#depthNormalMap#
end

effect knife -rigid -hardStop
	particles knife
end

