effect KanariaDemonLordtrack -hardStop
	sound -name DemonLord_Kanaria -stop #-loop
end

setf delay 5/30
sequence KanariaDemonLordtracktimed
	wait $delay
	play KanariaDemonLordtrack
end

effect KanariaDemonLordtracktimed
	sequence KanariaDemonLordtracktimed
end

effect Demon_Lord_Stage_Effect -hardStop
	effect KanariaDemonLordtracktimed 
end

export Demon_Lord_Stage_Effect